{
	"version": 3,
	"$schema": "https://schemas.wp.org/trunk/theme.json.",
	"settings": {
		"appearanceTools": true,
		"color": {
			"background": true,
			"customDuotone": false,
			"customGradient": true,
			"defaultGradients": false,
			"defaultPalette": false,
			"palette": [
				{
					"slug": "primary",
					"color": "#542988",
					"name": "Primary"
				},
				{
					"slug": "secondary",
					"color": "#54595f",
					"name": "Secondary"
				},
				{
					"slug": "accent",
					"color": "#7f5ea6",
					"name": "Accent"
				},
				{
					"slug":"body",
					"color":"#444444",
					"name":"Body"
				}
			],
			"text": true,
			"heading": true,
			"button": true
		},
		"dimensions": true,
		"layout": {
			"contentSize": "640px",
			"wideSize": "1200px"
		},
		"spacing": true,
		"typography": {
			"customFontSize": true,
			"defaultFontSizes": false,
			"lineHeight": true,
			"dropCap": false,
			"fluid": true,
			"fontStyle": true,
			"fontWeight": true,
			"letterSpacing": true,
			"textDecoration": true,
			"textTransform": true,
			"fontSizes": [
				{
					"slug": "xs",
					"size": "clamp(0.8rem, 0.941176vi + 0.56rem, 1.2rem)",
					"name": "XS"
				},
				{
					"slug": "base",
					"size": "clamp(1rem, 1.176471vi + 0.71rem, 1.5rem)",
					"name": "Base"
				},
				{
					"slug": "small",
					"size": "clamp(1.25rem, 1.482353vi + 0.88rem, 1.88rem)",
					"name": "Small (H6)"
				},
				{
					"slug": "medium",
					"size": "clamp(1.56rem, 1.835294vi + 1.1rem, 2.34rem)",
					"name": "Medium (H5)"
				},
				{
					"slug": "large",
					"size": "clamp(1.95rem, 2.305882vi + 1.37rem, 2.93rem)",
					"name": "Large (H4)"
				},
				{
					"slug": "xl",
					"size": "clamp(2.44rem, 2.870588vi + 1.72rem, 3.66rem)",
					"name": "XL (H3)"
				},
				{
					"slug": "2xl",
					"size": "clamp(3.05rem, 3.6vi + 2.15rem, 4.58rem)",
					"name": "3XL (H2)"
				},
				{
					"slug": "3xl",
					"size": "clamp(3.81rem, 4.494118vi + 2.69rem, 5.72rem)",
					"name": "3XL (H1)"
				}
			]
		},
		"custom": {
			"lineHeight": "calc(4px + 2ex)",
			"transitionTiming": ".15s",
			"transitionEasing": "ease-in-out",
			"transition": "var(--wp--custom--transition-timing) var(--wp--custom--transition-easing)",
			"gutters": "1.25rem"
		}
	},
	"styles": {
		"spacing": {
			"blockGap": "0"
		},
		"typography": {
			"fontSize": "var(--wp--preset--font-size--base)",
			"lineHeight": "var(--wp--custom--line-height)"
		},
		"blocks": {
			"core/paragraph": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--base)",
					"lineHeight": "var(--wp--custom--line-height)"
				}
			},
			"core/heading": {
				"typography": {
					"lineHeight": "var(--wp--custom--line-height)"
				}
			}
		},
		"elements": {
			"heading": {
				"lineHeight": "var(--wp--custom--line-height)"
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--3-xl)",
					"lineHeight": "var(--wp--custom--line-height)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--2-xl)",
					"lineHeight": "var(--wp--custom--line-height)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xl)",
					"lineHeight": "var(--wp--custom--line-height)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"lineHeight": "var(--wp--custom--line-height)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"lineHeight": "var(--wp--custom--line-height)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"lineHeight": "var(--wp--custom--line-height)"
				}
			}
		}
	}
}
